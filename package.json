    "lint-staged": "^7.1.3",
    "mini-css-extract-plugin": "^0.4.0",
    "prettier": "1.13.4",
    "serve": "^8.1.1",
    "short-uuid": "^3.0.0",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.3"
  }
